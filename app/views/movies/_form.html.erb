<%= render('layouts/errors', :object => @movie) %>

<%= form_for @movie do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :actors %>
  <%= f.text_field :actors %>
  <%= f.label :genre %>
  <%= f.text_field :genre %>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <%= f.submit %>
<% end %>
